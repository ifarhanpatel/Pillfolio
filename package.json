{
  "name": "pillfolio",
  "packageManager": "yarn@4.11.0",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "format": "prettier --write .",
    "test": "jest --watch",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci",
    "e2e": "detox test --configuration ios.sim.debug",
    "prebuild:ios": "expo prebuild --platform ios",
    "e2e:ios:build": "detox build --configuration ios.sim.debug",
    "e2e:ios:run": "detox test --configuration ios.sim.debug",
    "e2e:ios": "yarn prebuild:ios && yarn e2e:ios:build && yarn e2e:ios:run",
    "e2e:ios:build:ci": "detox build --configuration ios.sim.release",
    "e2e:ios:run:ci": "detox test --configuration ios.sim.release",
    "e2e:ios:ci": "yarn prebuild:ios && yarn e2e:ios:build:ci && yarn e2e:ios:run:ci",
    "prebuild:android": "expo prebuild --platform android",
    "e2e:android:build": "detox build --configuration android.emu.debug",
    "e2e:android:run": "detox test --configuration android.emu.debug",
    "e2e:android": "yarn prebuild:android && yarn e2e:android:build && yarn e2e:android:run",
    "e2e:ci": "yarn e2e:ios:run:ci"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.7",
    "expo-constants": "~18.0.8",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.4",
    "expo-splash-screen": "~31.0.10",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.27.7",
    "@babel/types": "^7.27.7",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^13.2.0",
    "@types/react": "~19.1.0",
    "detox": "^20.32.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.0",
    "prettier": "^3.3.3",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2"
  },
  "private": true
}
